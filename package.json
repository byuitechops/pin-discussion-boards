{
  "name": "pin-discussion-boards",
  "version": "1.0.0",
  "description": "Boards organize by most recently viewed, rather than in order. Pinning them keeps them in order. This child module pins the discussion boards in order, so they don't move around for the user.",
  "main": "main.js",
  "dependencies": {
    "canvas-wrapper": "github:byuitechops/canvas-wrapper"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "child-development-kit": "github:byuitechops/child-development-kit",
    "tap": "^10.7.3"
  },
  "scripts": {
    "start": "node ./node_modules/child-development-kit/runChild.js",
    "test": "tap ./node_modules/child-development-kit/runChild.js -T"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byuitechops/pin-discussion-boards.git"
  },
  "author": "Seth Childers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byuitechops/pin-discussion-boards/issues"
  },
  "homepage": "https://github.com/byuitechops/pin-discussion-boards#readme",
  "childType": "postImport"
}